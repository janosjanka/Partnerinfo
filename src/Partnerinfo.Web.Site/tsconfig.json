{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": false,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "skipDefaultLibCheck": true,
    "target": "es5"
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
